import math
#find prime numbers
def isPrime(x):
    for j in range(2, int(math.sqrt(x)+1)):
        if x % j == 0:
            return False
    return True

#prime number switcharoo
def circular(number):
    number = list(str(number))
    for i in range(len(number)):
        number += [number.pop(0)]
        a = int("".join(number))
        if not isPrime(a):
            return False
    return True

for i in range(2, 100):
    print(circular(i))
