import math
from random import randint
prime = []
circPrime = []
final = []
for i in range(2, 100):
  is_prime = True
  for j in range(2, int(math.sqrt(i)+1)):
    if i % j == 0:
      is_prime = False
    if is_prime:
      prime.append(i)
for item in prime:
    number = list(str(item))
    
for i in range(len(number) - 1):
    number += [number.pop(0)]
    circPrime.append("".join(number))
    print "".join(number)

for x in cirPrime:
    is_prime = True
  for j in range(2, int(math.sqrt(i)+1)):
    if i % j == 0:
      is_prime = False
    if is_prime:
      final.append(i)
