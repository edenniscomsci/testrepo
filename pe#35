from random import randint

val = randint(2, 4000000)
number = list(str(val))
circPrime = []
print number

for i in range(len(number) - 1):
    number += [number.pop(0)]
    circPrime.append("".join(number))
    print "".join(number)

print(circPrime)
print(len(circPrime))
from random import randint
import math
prime = []

def isPrime(i):
    is_prime = True
    for number in range (2,int(math.ceil(math.sqrt(i)) + 1)):
            if i % number == 0:
                prime.append(i)
      
isPrime()          
print(prime)
