import math
from random import randint
prime = []
random = []
circPrime = []
final = []
#find prime numbers
for i in range(2, 100):
  is_prime = True
  for j in range(2, int(math.sqrt(i)+1)):
    if i % j == 0:
      is_prime = False
  if is_prime:
        prime.append(i)
print(prime)
#making list of prime numbers
for item in prime:
    number = list(str(item))
    random.append(number)
print(random)
#prime number switcharoo
for digit in random:
    for i in range(len(number) - 1):
        number += [number.pop(0)]
        circPrime.append("".join(number))
        print (circPrime)
#testing if the circulars are prime
for x in circPrime:
    is_prime = True
    for j in range(2, int(math.sqrt(float(x)+1))):
        if float(x) % j == 0:
            is_prime = False
        if is_prime:
            final.append(float(x))
            print(final)
#print(final)
